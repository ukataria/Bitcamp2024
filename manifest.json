{
    "manifest_version": 3,
    "name": "Hello Pinterest",
    "version": "1.0",
    "description": "Add 'Hello' under images on Pinterest",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.pinterest.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  